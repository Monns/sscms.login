@page
@{ Layout = "_Layout"; }

<div style="height: 10px;"></div>

<el-alert type="info">
  请进入QQ 互联平台（https://connect.qq.com/） - 应用管理 - 网站应用 - 查看 获取APP ID及APP Key
</el-alert>

<div style="height: 10px;"></div>

<el-form ref="form" :model="form" size="small" status-icon label-width="260px">

  <el-form-item label="是否启用QQ登录">
    <el-radio v-model="form.isQq" :label="true">启用</el-radio>
    <el-radio v-model="form.isQq" :label="false">不启用</el-radio>
  </el-form-item>
  <el-form-item v-if="form.isQq" label="APP ID" prop="qqAppId" :rules="{ required: true, message: '请输入APP ID' }">
    <el-input v-model="form.qqAppId"></el-input>
  </el-form-item>
  <el-form-item v-if="form.isQq" label="APP Key" prop="qqAppKey" :rules="{ required: true, message: '请输入APP Key' }">
    <el-input v-model="form.qqAppKey"></el-input>
  </el-form-item>

</el-form>

<el-divider></el-divider>
<div style="height: 10px"></div>

<el-row>
  <el-col :span="24" align="center">
    <el-button type="primary" v-on:click="btnSubmitClick" size="small">保 存</el-button>
    <el-button v-if="url" type="primary" v-on:click="btnTestClick" size="small">测 试</el-button>
  </el-col>
</el-row>

@section Scripts{
  <script src="/assets/login/js/connectQq.js" type="text/javascript"></script>
}
